# CodeIgniter 4
#--------------------------------------------------------------------
# This file contains the default ignore rules for this project.
# If you keep those rules, your project will be compatible with
# the CodeIgniter 4 framework.
#--------------------------------------------------------------------

#--------------------------------------------------------------------
# OS generated files
#--------------------------------------------------------------------
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

#--------------------------------------------------------------------
# IDE generated files
#--------------------------------------------------------------------
.vscode/
.idea/
*.swp
*.swo
*~

#--------------------------------------------------------------------
# Logs
#--------------------------------------------------------------------
*.log
writable/logs/*.log

#--------------------------------------------------------------------
# Runtime data
#--------------------------------------------------------------------
writable/cache/*
!writable/cache/index.html
writable/session/*
!writable/session/index.html
writable/debugbar/*
!writable/debugbar/index.html

#--------------------------------------------------------------------
# Environment
#--------------------------------------------------------------------
.env
.env.*

#--------------------------------------------------------------------
# Composer
#--------------------------------------------------------------------
vendor/

#--------------------------------------------------------------------
# Node
#--------------------------------------------------------------------
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

#--------------------------------------------------------------------
# Testing
#--------------------------------------------------------------------
phpunit.xml
.phpunit.result.cache
coverage/

#--------------------------------------------------------------------
# Security
#--------------------------------------------------------------------
# Don't commit sensitive configuration files
app/Config/Database.php
app/Config/Email.php
app/Config/Encryption.php

#--------------------------------------------------------------------
# Temporary files
#--------------------------------------------------------------------
*.tmp
*.temp
